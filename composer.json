{
  "name": "leroy-merlin-br/exacttarget-client",
  "description": "Easy way to interact with ExactTarget REST API in Laravel",
  "keywords": ["exacttarget", "laravel", "salesforce", "rest", "api", "sdk"],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "illuminate/config": "^9.0 || ^10.0",
    "illuminate/support": "^9.0 || ^10.0",
    "guzzlehttp/guzzle": "^7.4.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "LeroyMerlin\\ExactTarget\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LeroyMerlin\\ExactTarget\\": "tests/"
    }
  }
}
